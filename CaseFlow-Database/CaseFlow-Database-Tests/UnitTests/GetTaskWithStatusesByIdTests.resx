  <data name="GetTaskWithStatusesById_Returns_Tasks_TestAction.SqlScript" xml:space="preserve">
    <value>/*
Add SQL statements here to exercise the database object 
that you want to test.

To test a stored procedure, invoke it here by adding 
an EXEC statement that has appropriate parameters.

In the lower pane, you can add test conditions that verify 
whether the results of your SQL statements match what 
you expect.
*/
DECLARE @insertedRoleIds		TABLE(Id INT)
DECLARE	@insertedTaskIds		TABLE(Id INT)
		

DECLARE @caseworkerRoleID	INT,
		@taskId				INT,
		@caseworkerId		INT,
		@caseCreatedStatus	NVARCHAR(256) = 'Case Created';

INSERT INTO [caseFlow].[CaseworkerRole] ([Name]) 
OUTPUT								INSERTED.Id INTO @insertedRoleIds
VALUES ('Caseworker Manager');

SELECT	@caseworkerRoleID = Id FROM @insertedRoleIds;

EXEC	[caseFlow].[CreateUser]
		@caseworkerRoleId = @caseworkerRoleID,
		@forename = N'Bobby',
		@surname = N'Roode',
		@email = N'b.roode@hmcts.org.uk',
		@passwordHash = 0x3c762fa9a0b692081addd720be34c0f6dc93085b6e2c8b8d1d57564bb71273f5,
		@passwordSalt =  0x5fd674733fca5087591c9aff6f89629f,
		@createdDateTime = '2025-09-07 22:02:00';

SELECT TOP 1 @caseworkerId = Id
FROM [caseFlow].[Caseworker]
ORDER BY Id DESC

INSERT INTO [caseFlow].[Task]					(Title, [Description], DueDateTime)
		OUTPUT											INSERTED.Id INTO @insertedTaskIds
		VALUES											('J-Walking', 'Perpetrator was walking into onco,ing Traffic', '2025-12-07 22:02:00')

		SELECT		@taskId = Id FROM @insertedTaskIds;

		INSERT INTO [caseFlow].[TaskStatus]				(TaskId, StatusId, CaseworkerId, Notes, LogDateTime)
		SELECT											@taskId, s.Id, @caseworkerId, '', GETDATE()
												FROM	[caseFlow].[Status] s
												WHERE	Title = @caseCreatedStatus

EXEC [caseFlow].[GetTaskWithStatusesById]
@taskId = @taskId;</value>
  </data>
